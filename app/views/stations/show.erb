<div id ='station_id' ><h1><%= @station.name %></h1></div>
<h2>Info</h2>
<p>City <%= @station.city %></p>
<p>Date Installed <%= @station.installation_date %></p>
<p>Dock Count <%= @station.dock_count %></p>
<a href="/stations/<%= @station.id %>/edit"><input type="submit" value="Edit Station" /></a>
<form action="/stations/<%= @station.id %>" method="post">
  <input type="hidden" name="_method" value="DELETE" />
  <input type="submit" value="Delete Station" />
</form>
